{
  "name": "@ats/backend",
  "version": "1.0.0",
  "description": "ATS Backend API with Express and Couchbase",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon --watch src --exec ts-node src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "lint": "eslint src --ext .ts",
    "seed": "ts-node src/scripts/seed.ts",
    "clear-db": "ts-node src/scripts/clearDb.ts"
  },
  "dependencies": {
    "@ats/shared": "file:../shared",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
